SELECT distinct * WHERE {
  
  
  
service <https://dbpedia.org/sparql> {

    
 <http://dbpedia.org/resource/On_the_Road> dbo:literaryGenre ?genre .
 ?genre dbo:wikiPageWikiLink ?associated_with .
    ?associated_with dbp:occupation ?occ .
   FILTER(?occ = "novelist"@en || ?occ = "Writer"@en ).  

     ?beat_works dbo:author ?associated_with.
    
    
    
    }
} 
